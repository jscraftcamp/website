    "@babel/preset-react": "^7.9.1",
    "babel-plugin-remove-graphql-queries": "^2.8.0",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "gatsby": "2.23.1",
    "gatsby-image": "2.4.6",
    "gatsby-plugin-manifest": "2.4.10",
    "gatsby-plugin-offline": "3.2.8",
    "gatsby-plugin-react-helmet": "3.3.3",
    "gatsby-plugin-sass": "2.3.3",
    "gatsby-plugin-sharp": "2.6.10",
    "gatsby-source-filesystem": "2.3.10",
    "gatsby-transformer-json": "2.4.4",
    "gatsby-transformer-sharp": "2.5.4",
    "modern-normalize": "^0.6.0",
    "netlify-plugin-gatsby-cache": "0.2.2",
    "node-sass": "^4.13.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "babel-loader": "^8.1.0",
    "hamjest": "3.2.3",
    "mocha": "6.1.4",
    "prettier": "1.19.1",
    "recursive-readdir-sync": "1.0.6"
  },
  "scripts": {
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "prebuild": "rm -rfv dist ; mkdir -p ./public",
    "build": "cp CNAME public/; npm run build:participants.json ; gatsby build;",
    "build:participants.json": "node scripts/concat-json.js ./participants public/participants.json; cp ./public/participants.json ./src/",
    "test": "mocha test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
