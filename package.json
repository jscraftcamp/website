{
  "name": "js-craftcamp-website",
  "version": "2.0.0",
  "description": "Website for the JSCC (jscraftcamp.org)",
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "babel-preset-gatsby": "2.5.2",
    "classnames": "2.3.1",
    "css-loader": "5.2.7",
    "gatsby": "4.5.5",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-manifest": "4.5.2",
    "gatsby-plugin-offline": "5.5.2",
    "gatsby-plugin-react-helmet": "5.5.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-sass": "5.5.0",
    "gatsby-plugin-sharp": "4.5.2",
    "gatsby-source-filesystem": "4.5.2",
    "gatsby-transformer-json": "4.5.0",
    "gatsby-transformer-sharp": "4.5.0",
    "modern-normalize": "1.1.0",
    "netlify-plugin-gatsby-cache": "0.3.2",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "sass-loader": "8.0.2",
    "sass-mq": "6.0.0",
    "style-loader": "3.3.1",
    "svg-sprite-loader": "6.0.11",
    "svgo-loader": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@storybook/addon-actions": "6.4.13",
    "@storybook/addon-links": "6.4.13",
    "@storybook/addon-viewport": "6.4.13",
    "@storybook/addons": "6.4.13",
    "@storybook/builder-webpack5": "6.4.13",
    "@storybook/manager-webpack5": "6.4.13",
    "@storybook/react": "6.4.13",
    "babel-loader": "8.2.3",
    "babel-plugin-remove-graphql-queries": "4.5.2",
    "hamjest": "3.7.3",
    "hygen": "6.1.0",
    "mocha": "9.1.4",
    "prettier": "2.5.1",
    "recursive-readdir-sync": "1.0.6",
    "sass": "1.49.0",
    "sass-resources-loader": "2.2.4"
  },
  "scripts": {
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "prebuild": "rm -rfv dist ; mkdir -p ./public",
    "build": "cp CNAME public/; npm run build:participants.json ; gatsby build;",
    "build:participants.json": "node scripts/concat-json.js ./participants public/participants.json; cp ./public/participants.json ./src/",
    "test": "mocha test",
    "build-storybook": "NODE_ENV=test build-storybook",
    "storybook": "NODE_ENV=test start-storybook -p 6006 -s ./public",
    "prestorybook": "npm run copy-static-queries",
    "prebuild-storybook": "npm run copy-static-queries",
    "copy-static-queries": "cp -r ./public/page-data/sq/d ./public/static",
    "hygen": "hygen"
  },
  "author": "Robert Hostlowsky",
  "license": "MIT",
  "repository": "https://github.com/jscraftcamp/website.git"
}
