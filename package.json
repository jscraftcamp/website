{
  "name": "js-craftcamp-website",
  "version": "2.0.0",
  "description": "Website for the JSCC (jscraftcamp.org)",
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "babel-preset-gatsby": "1.9.0",
    "classnames": "2.3.1",
    "css-loader": "5.2.7",
    "gatsby": "3.9.1",
    "gatsby-image": "3.9.0",
    "gatsby-plugin-manifest": "3.9.0",
    "gatsby-plugin-offline": "4.9.0",
    "gatsby-plugin-react-helmet": "4.9.0",
    "gatsby-plugin-react-svg": "3.0.1",
    "gatsby-plugin-sass": "4.9.0",
    "gatsby-plugin-sharp": "3.9.0",
    "gatsby-source-filesystem": "3.9.0",
    "gatsby-transformer-json": "3.9.0",
    "gatsby-transformer-sharp": "3.9.0",
    "modern-normalize": "1.1.0",
    "netlify-plugin-gatsby-cache": "0.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "sass-loader": "8.0.2",
    "sass-mq": "5.0.1",
    "style-loader": "2.0.0",
    "svg-sprite-loader": "6.0.9",
    "svgo-loader": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@storybook/addon-actions": "6.3.4",
    "@storybook/addon-links": "6.3.4",
    "@storybook/addon-viewport": "6.3.4",
    "@storybook/addons": "6.3.4",
    "@storybook/react": "6.3.4",
    "babel-loader": "8.2.2",
    "babel-plugin-remove-graphql-queries": "3.9.0",
    "hamjest": "3.7.2",
    "hygen": "6.1.0",
    "mocha": "8.4.0",
    "node-sass": "4.14.1",
    "prettier": "2.3.2",
    "recursive-readdir-sync": "1.0.6",
    "sass-resources-loader": "2.2.3"
  },
  "scripts": {
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "prebuild": "rm -rfv dist ; mkdir -p ./public",
    "build": "cp CNAME public/; npm run build:participants.json ; gatsby build;",
    "build:participants.json": "node scripts/concat-json.js ./participants public/participants.json; cp ./public/participants.json ./src/",
    "test": "mocha test",
    "build-storybook": "NODE_ENV=test build-storybook",
    "storybook": "NODE_ENV=test start-storybook -p 6006 -s ./public",
    "prestorybook": "npm run copy-static-queries",
    "prebuild-storybook": "npm run copy-static-queries",
    "copy-static-queries": "cp -r ./public/page-data/sq/d ./public/static",
    "hygen": "hygen"
  },
  "author": "Robert Hostlowsky",
  "license": "MIT",
  "repository": "https://github.com/jscraftcamp/website.git"
}
