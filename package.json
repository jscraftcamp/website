    "gatsby-plugin-sharp": "2.6.10",
    "gatsby-plugin-svg-sprite": "2.0.1",
    "gatsby-source-filesystem": "2.3.10",
    "gatsby-transformer-json": "2.4.4",
    "gatsby-transformer-sharp": "2.5.4",
    "modern-normalize": "1.0.0",
    "netlify-plugin-gatsby-cache": "0.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "sass-loader": "8.0.2",
    "sass-mq": "5.0.1",
    "style-loader": "2.0.0",
    "svg-sprite-loader": "5.2.1",
    "svgo-loader": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.13.5",
    "@babel/preset-react": "7.12.13",
    "@storybook/addon-actions": "6.1.20",
    "@storybook/addon-links": "6.1.20",
    "@storybook/addon-viewport": "6.1.20",
    "@storybook/addons": "6.1.20",
    "@storybook/react": "6.1.20",
    "babel-loader": "8.2.2",
    "babel-plugin-remove-graphql-queries": "2.9.5",
    "hamjest": "3.7.1",
    "hygen": "6.0.4",
    "mocha": "8.3.2",
    "node-sass": "4.14.1",
    "prettier": "2.2.1",
    "recursive-readdir-sync": "1.0.6",
    "sass-resources-loader": "2.1.1"
  },
  "scripts": {
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "prebuild": "rm -rfv dist ; mkdir -p ./public",
    "build": "cp CNAME public/; npm run build:participants.json ; gatsby build;",
    "build:participants.json": "node scripts/concat-json.js ./participants public/participants.json; cp ./public/participants.json ./src/",
    "test": "mocha test",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "hygen": "hygen"
  },
  "author": "Robert Hostlowsky",
  "license": "MIT",
  "repository": "https://github.com/jscraftcamp/website.git"
}
