{
  "name": "js-craftcamp-website",
  "version": "2.0.0",
  "description": "Website for the JSCC (jscraftcamp.org)",
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "css-loader": "5.1.0",
